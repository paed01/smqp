{
  "name": "smqp",
  "version": "6.2.0",
  "description": "Synchronous message queueing package",
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "main": "dist/index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/smqp"
  },
  "bugs": {
    "url": "https://github.com/paed01/smqp/issues"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "src",
    "dist/src",
    "dist/index.js",
    "index.js"
  ],
  "scripts": {
    "test": "mocha",
    "test:lcov": "nyc mocha && nyc report --reporter lcov && npm run posttest",
    "posttest": "npm run dist &&  eslint . --cache && npm run toc",
    "cov:html": "nyc mocha && nyc report --reporter=html",
    "dist": "babel index.js -d dist && babel src/**.js -d dist/src",
    "prepare": "npm run dist",
    "toc": "node ./scripts/generate-api-toc.js"
  },
  "keywords": [
    "messaging",
    "broker",
    "routing",
    "exchange",
    "queue",
    "fifo",
    "shovel",
    "publish",
    "subscribe",
    "ack",
    "nack",
    "reject",
    "topic",
    "direct",
    "shovel"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "chai": "^4.3.6",
    "chronokinesis": "^4.0.1",
    "eslint": "^8.22.0",
    "markdown-toc": "^1.2.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "exclude": [
      "dist",
      "test"
    ]
  }
}
